  
  
  <table>
  <caption>Доступные курсы валют</caption>
  <tr>
    <th>Код</th>
    <th>Название</th>
    <th>Курс</th>
  </tr>
  <%= for currency <- @currencies do %>
    <tr>
      <td><%= currency["CharCode"] %></td>
      <td><%= currency["Name"] %></td>
      <td><%= currency["Value"] %> рублей</td>
    </tr>
  <% end %>
</table>